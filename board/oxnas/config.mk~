#
# image should be loaded at 0x01000000
#

TEXT_BASE = 0x48d00000
# Define the master clock frequencies of the NAS chip - allow overriding from make invocation cmd line
    PLL400 ? = 400000000
    RPSCLK ? = 25000000
    TEST_BRD ? = 0
    MEM_64M ? = 0
    PROBE_MEM_SIZE ? = 0
    PLATFORM_CPPFLAGS += -DPROBE_MEM_SIZE = $(PROBE_MEM_SIZE)
    - DHAVE_64M = $(MEM_64M)
    - DIS_TEST_BRD = $(TEST_BRD)
    - DNOMINAL_PLL400_FREQ = $(PLL400)
    - DNOMINAL_RPSCLK_FREQ = $(RPSCLK)
